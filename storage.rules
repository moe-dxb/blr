rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {

    match /receipts/{uid}/{claimId}/{fileName} {
      allow read, write: if request.auth != null && request.auth.uid == uid;
    }

    match /docs/{uid}/{docId} {
      allow read, write: if request.auth != null && request.auth.uid == uid;
    }

    // default deny
    match /{allPaths=**} { allow read, write: if false; }
  }
}